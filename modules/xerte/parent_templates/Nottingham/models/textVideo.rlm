	<PG title="Text Video" name="textVideo">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();if (rootIcon.projMode == "dev"){   // Page Wizard  pageIcon.templateData = pageIcon.extXML.toObject();  pageIcon.templateData = pageIcon.templateData.pageWizard[0];} else {  // Toolkits  myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);  pageIcon.templateData = myXML.toObject();}  rootIcon.setTitleText(templateData.learningObject[0].name);  // handle multiple page data definitions in a project if (rootIcon.projectPageIndex == undefined){     rootIcon.projectPageIndex = 0;}// check if this page has a definition for the current project index and default to 0 if notif (pageIcon.templateData.textVideo[rootIcon.projectPageIndex].name == undefined){  projPI = 0;} else {  projPI = rootIcon.projectPageIndex; }  // set up text stringsif (pageIcon.templateData.textVideo[projPI].transcriptTabTxt != undefined){ transcriptTabTxt = pageIcon.templateData.textVideo[projPI].transcriptTabTxt;} else {  transcriptTabTxt = "Transcript";}if (pageIcon.templateData.textVideo[projPI].transcriptTabTip != undefined){ transcriptTabTip = pageIcon.templateData.textVideo[projPI].transcriptTabTip;} else {  transcriptTabTip = "Transcript";}if (pageIcon.templateData.textVideo[projPI].transcriptTabWidth != undefined){ transcriptTabWidth = pageIcon.templateData.textVideo[projPI].transcriptTabWidth;} else {  transcriptTabWidth = "90";}//size the movieif (pageIcon.templateData.textVideo[projPI].movieSize != undefined){  icon.parentNode.getChildByName('movie').attributes.w = pageIcon.templateData.textVideo[projPI].movieSize.split(',')[0];  icon.parentNode.getChildByName('movie').attributes.h = pageIcon.templateData.textVideo[projPI].movieSize.split(',')[1];}vidURL = pageIcon.templateData.textVideo[projPI].url;//support for links from moletvif (vidURL.indexOf('moletv.org.uk') != -1){  vid = vidURL.split('=')[1];  vidURL = "'http://www.moletv.org.uk/mod/FileStream.aspx?v=" + vid + "&t=1&z=.flv'"}if (pageIcon.templateData.textVideo[projPI].narration != undefined){  narration = pageIcon.templateData.textVideo[projPI].narration;  rootIcon.broadcast('showAudio');}fullScreen = false;]]></SCR>		<DIS tabIndex="-1" id="panel" name="panel" x="-300" y="20" type="ext" url="templatePath + 'common/whitePanel.swf'"><![CDATA[]]></DIS>		<INT name="loadPanel" perpetual="0">			<RES name="onLoad" type="event" eventName="onLoad" icon="panel" erase="1" exit="1">				<SCR name="calculate"><![CDATA[//ok to move on]]></SCR>			</RES>		</INT>		<MOV id="movie" name="movie" x="0" y="30" url="{vidURL}" controls="1" play="0"><![CDATA[{pageIcon.templateData.textVideo[projPI].tip}]]></MOV>		<INT name="loadVideo" perpetual="0">			<RES name="onMovieReady" type="event" eventName="onMovieReady" icon="movie" erase="1" exit="1">				<SCR name="calculate"><![CDATA[//resize the panel to the graphicpanel._width = movie._width + 20;panel._height = movie._height + 40;if (pageIcon.templateData.textVideo[projPI].align == 'Left'){  panel._x = stageWidth - panel._width - 20;  movie._x = panel._x + 10;  textX = 20;} else {  panel._x = 20;  movie._x = 30;  textX = panel._width + 40;}textWidth = stageWidth - 60 - panel._width;if (pageIcon.templateData.textVideo[projPI].reset == 'false'){  movie.flvPlayer.autoRewind = false;}initW = movie._width;initH = movie._height;initX = movie._x;initY = movie._y;if (pageIcon.templateData.textVideo[projPI].startFrame != undefined){  movie.flvPlayer.seek(Number(pageIcon.templateData.textVideo[projPI].startFrame));}]]></SCR>			</RES>		</INT>		<DIS id="textBlock" scrolling="auto" name="text" x="textX" y="20" w="textWidth" h="480" type="text"><![CDATA[{pageIcon.templateData.textVideo[projPI]}]]></DIS>		<INT name="events" perpetual="1">			<RES rs="btnToggle" swf="templatePath + 'common/zoom.png'" name="toggle" type="button" x="movie._x + movie._width - 20" y="movie._y + movie._height + 2" label="label" erase="1" exit="0">				<SCR name="[Untitled]"><![CDATA[if (fullScreen == false){  fullScreen = true;  playOn = false;  if (movie.flvPlayer.playing == true){    movie.pause();    playOn = true;  }  movie.setSize(800,434);  if (playOn == true){    movie.play();  }    if (movie.flvPlayer.playing == true){    }  textBlock._visible = false;  } else {  fullScreen = false;  playOn = false;  if (movie.flvPlayer.playing == true){    movie.pause();    playOn = true;  }    movie.setSize(initW,initH);      if (playOn == true){    movie.play();  }  textBlock._visible = true;}]]></SCR>			</RES>			<RES name="onResize" type="event" eventName="onResize" icon="movie" erase="1" exit="0">				<SCR name="[Untitled]"><![CDATA[if (fullScreen == true){  movie._x = 400 - movie._width / 2;        panel._x = movie._x - 10;  panel._y = movie._y - 10;  panel._width = movie._width + 20;  panel._height = movie._height +40;      btnToggle._x = movie._x + movie._width - 20;  btnToggle._y = movie._y + movie._height + 2;} else {  movie._x = initX;  movie._y = initY;    panel._x = movie._x - 10;  panel._y = movie._y - 10;  panel._width = initW + 20;  panel._height = initH +40;        btnToggle._x = initX + initW - 20;  btnToggle._y = initY + initH + 2;}]]></SCR>			</RES>			<RES name="onPlayheadUpdate" type="event" eventName="onPlayheadUpdate" icon="movie" erase="1" exit="0">				<SCR name="[Untitled]"><![CDATA[//end time reached?if (pageIcon.templateData.textVideo[projPI].endFrame != undefined){  if (movie.flvPlayer.playheadTime > Number(pageIcon.templateData.textVideo[projPI].endFrame)){    movie.pause();    movie.flvPlayer.seek(Number(pageIcon.templateData.textVideo[projPI].startFrame));  }}]]></SCR>			</RES>		</INT>		<SCR name="setupTranscript"><![CDATA[if (pageIcon.templateData.textVideo[projPI].transcript == undefined){  icon.nextSibling.removeNode();}]]></SCR>		<INT id="trans" name="transcript" perpetual="0">			<RES toolTip="{transcriptTabTip}" w="transcriptTabWidth" swf="templatePath + 'common/invertTab.swf'" rs="btnTrans" name="showTranscript" type="button" x="panel._x + panel._width - (10 + parseInt(transcriptTabWidth)" y="panel._y + panel._height" label="{transcriptTabTxt}" erase="1" exit="0">				<SCR name="tween"><![CDATA[if (transShow != true){  panel.tween("_height", panel._height, 464,0.2,'Strong', 'easeIn');  btnTrans.tween("_y", btnTrans._y, 484,0.2,'Strong', 'easeIn');  transShow = true;} else {  panel.tween("_height", panel._height, movie._height + 30,0.2,'Strong', 'easeIn');  btnTrans.tween("_y", btnTrans._y, movie._y + movie._height + 20,0.2,'Strong', 'easeIn');  transShow = false;}]]></SCR>			</RES>			<RES name="onMotionFinished" type="event" eventName="onMotionFinished" icon="panel" erase="1" exit="0">				<SCR name="broadcast"><![CDATA[if (transShow == true){  trans.broadcast("showTranscript");}]]></SCR>			</RES>			<RES name="showTranscript" type="event" eventName="showTranscript" icon="trans" erase="1" exit="0">				<DIS textColour="#333333" scrolling="auto" name="transcript" x="movie._x" y="movie._y + movie._height" w="movie._width" h="panel._height - movie._height - 20" type="text"><![CDATA[{pageIcon.templateData.textVideo[projPI].transcript}]]></DIS>			</RES>		</INT>	</PG>