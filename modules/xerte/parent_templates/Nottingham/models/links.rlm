	<PG title="Links" name="links">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();if (rootIcon.projMode == "dev"){   // Page Wizard  pageIcon.templateData = pageIcon.extXML.toObject();  pageIcon.templateData = pageIcon.templateData.pageWizard[0];} else {  // Toolkits  myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);  pageIcon.templateData = myXML.toObject();}  rootIcon.setTitleText(templateData.learningObject[0].name);  // handle multiple page data definitions in a project if (rootIcon.projectPageIndex == undefined){     rootIcon.projectPageIndex = 0;}// check if this page has a definition for the current project index and default to 0 if notif (pageIcon.templateData.links[rootIcon.projectPageIndex].name == undefined){  projPI = 0;} else {  projPI = rootIcon.projectPageIndex; } cols = Number(pageIcon.templateData.links[projPI].cols);cellH = Number(pageIcon.templateData.links[projPI].height) + (STYLES.defaultSize - 12)*3;if (pageIcon.templateData.links[projPI].narration != undefined){  narration = pageIcon.templateData.links[projPI].narration;  rootIcon.broadcast('showAudio');}]]></SCR>		<DIS id="maintext" scrolling="auto" name="text" x="20" y="20" w="550" h="475" type="text"><![CDATA[{pageIcon.templateData.links[projPI].text}]]></DIS>		<SCR name="makeLinks"><![CDATA[linkCount = pageIcon.templateData.links[projPI].link.length;if (icon.built != true){  //make the links  icon.nextSibling.firstChild.firstChild.duplicate(linkCount - 1);    for (i = 0; i < linkCount; i++){    icon.nextSibling.firstChild.childNodes[i].attributes.id = 'link' + i;  }    //make the hotobjects  icon.nextSibling.firstChild.lastChild.firstChild.duplicate(linkCount - 1);    for (i = 0; i < linkCount; i++){    icon.nextSibling.firstChild.lastChild.childNodes[i].attributes.rs = 'link' + i;  }  icon.built = true;}]]></SCR>		<DEC name="links" repeat="all" branch="sequence" pause="0" erase="1" eraseOnExit="0">			<PG name="links">				<DIS id="link0" name="protoLink" x="20 + Math.floor(icon.index() % cols) * (760 / cols)" y="maintext._y + maintext._height + 20 +Math.floor(icon.index() / cols) * cellH" w="(760 / cols) - 20" h="10" type="text"><![CDATA[{'<b>' + pageIcon.templateData.links[projPI].link[icon.index()].name + '</b><br>' + pageIcon.templateData.links[projPI].link[icon.index()].text + '<br><a href="' + pageIcon.templateData.links[projPI].link[icon.index()].url + '" target="_blank">' + pageIcon.templateData.links[projPI].link[icon.index()].url  + '</a>'}]]></DIS>				<SCR name="loadCSS"><![CDATA[// now the links have been created I can load the style sheetlinkArray = icon.parentNode.findChildNodes('name','protoLink');  for (i = 0; i < linkCount; i++){    linkArray[i].clip().loadCSS(templatePath + 'common/styles.css');  }]]></SCR>				<INT name="mainInteraction" perpetual="0">					<RES name="protoHotObject" type="hotObject" rs="link0" click="1" cursor="1" erase="1" exit="0">						<SCR name="getURL"><![CDATA[getURL(pageIcon.templateData.links[projPI].link[icon.parentNode.index()].url);]]></SCR>					</RES>				</INT>			</PG>		</DEC>	</PG>