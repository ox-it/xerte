	<LO version="1.0" targetFolder="Rss" description="A template for creating and managing RSS feeds" selectableText="1" templateData="FileLocation + 'rss.xml'" name="RSS Feed">		<IFC name="Interface" title="RSS Feed" visuals="1">			<PG name="rss">				<DIS tabIndex="-1" id="panel" h="474" w="500" name="panel" x="align == 'Left' ? 20 : 280" y="20" type="ext" url="templatePath + 'common/whitePanel.swf'"><![CDATA[]]></DIS>				<SCR name="parseFeed"><![CDATA[rootIcon.hideControls();rootIcon.setPageTitle(templateData.rss[0].channel[0].title);if (engine.extXML.firstChild.nodeName.indexOf('rss') != -1){  arr = engine.extXML.findNodesByName('item');  t = "<br>";  for (i = 0; i < arr.length; i++){    tempItem = arr[i].toObject();    t += '<font color="#0000FF"><b><a href="' + tempItem.link + '" target="_blank">' + tempItem.title + '</a></b></font><br><font color="#333333">' + tempItem.description + '</font><br><br>';  }}if (engine.extXML.firstChild.nodeName.indexOf('feed') != -1){    arr = engine.extXML.findNodesByName('entry');    t = "<br>";  for (i = 0; i < arr.length; i++){    tempItem = arr[i].toObject();    t += '<font color="#0000FF"><b><a href="' + tempItem.link[0].href + '" target="_blank">' + tempItem.title + '</a></b></font><br>';    if (tempItem.summary != undefined){      t+= '<font color="#333333">' + tempItem.summary + '</font><br><br>';    } else {      t+= '<br>';    }  }}]]></SCR>				<DIS scrolling="auto" name="feedText" x="panel._x + 20" y="panel._y" w="480" h="474" type="text"><![CDATA[{'<br>' + t}]]></DIS>				<DIS id="channelInfo" name="channelTitleDescription" x="20" y="20" w="240" h="10" type="text"><![CDATA[{'<b>' + templateData.rss[0].channel[0].title + '</b><br>' + templateData.rss[0].channel[0].description}]]></DIS>				<DIS id="urltext" textSize="10" name="urltext" x="55" y="464" w="240" h="10" type="text"><![CDATA[To copy the URL to this feed to your clipboard, click this icon.]]></DIS>				<DIS id="url" name="url" x="20" y="467" type="ext" url="templatePath + 'common/rss.png'"><![CDATA[]]></DIS>				<INT name="copyToClipboard" perpetual="0">					<RES name="onRelease" type="event" eventName="onRelease" icon="url" erase="1" exit="0">						<SCR name="copy"><![CDATA[System.setClipboard(_level0.site_url + FileLocation + 'data.xml');fscommand('messageBox', 'Feed URL copied to clipboard');]]></SCR>					</RES>				</INT>			</PG>		</IFC>		<BIN name="Deleted Items" />	</LO>