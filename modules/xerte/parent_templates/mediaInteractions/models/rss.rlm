gth; i++){    tempItem = arr[i].toObject();    t += '<font color="#0000FF"><b><a href="' + tempItem.link[0].href + '" target="_blank">' + tempItem.title + '</a></b></font><br>';    if (tempItem.summary != undefined){      t+= '<font color="#333333">' + tempItem.summary + '</font><br><br>';    } else {      t+= '<br></p>';    }  }}]]></SCR>			</RES>		</INT>		<DIS scrolling="auto" name="feedText" x="380" y="instr._y + instr._height + 20" w="400" h="514 - 60 - instr._height" type="text"><![CDATA[{'<br>' + t}]]></DIS>	</PG>