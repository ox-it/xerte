	<PG name="bullets">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();rootIcon.setTitleText(templateData.learningObject[0].name);bullets = pageIcon.templateData.bullets[0].split('\r\r');if (icon.attributes.built != 'true'){  icon.nextSibling.firstChild.duplicate(bullets.length - 1);  icon.attributes.built = 'true';}nextY = 20;if (pageIcon.templateData.bullets[0].image != undefined){  fLoadImage(pageIcon.templateData.bullets[0].image);} else {  fShowMedia(pageIcon.templateData.bullets[0].showMedia);}//helpif (pageIcon.templateData.bullets[0].help != undefined){  helpStr = pageIcon.templateData.bullets[0].help;  helpIcon._visible = true;} else {  helpIcon._visible = false;}]]></SCR>		<DEC name="bullets" repeat="all" branch="sequence" pause="0" erase="0" eraseOnExit="0">			<PG name="protoBullet">				<DIS id="b" transition="wipe('in',0.3,'None','easeNone',4)" scrolling="auto" name="bullet" x="380" y="nextY" w="400" h="475" type="text"><![CDATA[{bullets[icon.parentNode.index()]}]]></DIS>				<SCR name="getNextY"><![CDATA[nextY = b._y + b._height + 10;]]></SCR>				<INT name="pause" perpetual="0">					<RES name="onMotionFinished" type="event" eventName="onMotionFinished" icon="b" erase="1" exit="1">						<SCR name="exit"><![CDATA[//nothing to see here]]></SCR>					</RES>				</INT>			</PG>		</DEC>	</PG>