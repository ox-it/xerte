	<PG name="mcq">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();rootIcon.setTitleText(templateData.learningObject[0].name);align = pageIcon.templateData.mcq[0].align;panelWidth = pageIcon.templateData.mcq[0].panelWidth;if (panelWidth == "Small"){  panelW = 300;  textW = 440;} else if (panelWidth == "Medium"){  panelW = 450;  textW = 290;} else if (panelWidth == "Large"){  panelW = 600;  textW = 140;} else {  panelW = 760;  textW = 0;}if (pageIcon.templateData.mcq[0].image != undefined){  fLoadImage(pageIcon.templateData.mcq[0].image);} else {  fShowMedia(pageIcon.templateData.mcq[0].showMedia);}//helpif (pageIcon.templateData.mcq[0].help != undefined){  helpStr = pageIcon.templateData.mcq[0].help;  helpIcon._visible = true;} else {  helpIcon._visible = false;}]]></SCR>		<DIS id="txt" name="instruction" x="380" y="20" w="400" h="10" type="text"><![CDATA[{pageIcon.templateData.mcq[0].instruction}]]></DIS>		<DIS id="prpt" name="question" x="380" y="txt._y + txt._height + 20" w="400" h="10" type="text"><![CDATA[{pageIcon.templateData.mcq[0].prompt}]]></DIS>		<SCR name="makeOptions"><![CDATA[ln = pageIcon.templateData.mcq[0].option.length;optionTracking = new Array();if (icon.attributes.built != "true"){  icon.nextSibling.childNodes[1].duplicate(pageIcon.templateData.mcq[0].option.length - 2);  icon.attributes.built = "true";}//multi answer?if (pageIcon.templateData.mcq[0].type == 'Multiple Answer'){  for (i = 0; i < pageIcon.templateData.mcq[0].option.length; i++){    icon.nextSibling.childNodes[i].attributes.type="checkBox";  }}for (i = 0; i < ln; i++){  optionTracking[i] = 0;  if (pageIcon.templateData.mcq[0].option[i].correct == 'true'){    icon.nextSibling.childNodes[i].attributes.correct = 1;  } else {    icon.nextSibling.childNodes[i].attributes.correct = 0;  }}]]></SCR>		<INT id="ques" name="buttonInteraction" perpetual="0">			<RES w="400" name="protoOption" type="radio" x="380" y="prpt._y + prpt._height + 30" label="{pageIcon.templateData.mcq[0].option[icon.index()].text}" group="grp" erase="1" exit="0">				<SCR name="trackAnswers"><![CDATA[//track the questionif (pageIcon.templateData.mcq[0].type == 'Single Answer'){  for (i = 0; i < ln; i++){    optionTracking[i] = 0;  }  optionTracking[icon.parentNode.index()] = 1;} if (pageIcon.templateData.mcq[0].type == 'Multiple Answer'){  if (icon.parentNode.clip().getValue() == true){    optionTracking[icon.parentNode.index()] = 1;  } else {    optionTracking[icon.parentNode.index()] = 0;  }} btnSubmit.setEnabled(true);fb = pageIcon.templateData.mcq[0].option[icon.parentNode.index()].feedback;]]></SCR>			</RES>			<RES w="400" name="protoOption" type="radio" x="380" y="_ytile + 10" label="{pageIcon.templateData.mcq[0].option[icon.index()].text}" group="grp" erase="1" exit="0">				<SCR name="trackAnswers"><![CDATA[//track the questionif (pageIcon.templateData.mcq[0].type == 'Single Answer'){  for (i = 0; i < ln; i++){    optionTracking[i] = 0;  }  optionTracking[icon.parentNode.index()] = 1;} if (pageIcon.templateData.mcq[0].type == 'Multiple Answer'){  if (icon.parentNode.clip().getValue() == true){    optionTracking[icon.parentNode.index()] = 1;  } else {    optionTracking[icon.parentNode.index()] = 0;  }} btnSubmit.setEnabled(true);fb = pageIcon.templateData.mcq[0].option[icon.parentNode.index()].feedback;]]></SCR>			</RES>			<RES enabled="0" rs="btnSubmit" swf="templatePath + 'common/button.swf'" name="button" type="button" x="prpt._x" y="_ytile + 10" label="Check" erase="1" exit="0">				<SCR name="makeFeedback"><![CDATA[//single answer feedbackfb = "";for (i = 0; i < ln; i++){  if (optionTracking[i] == 1){    fb += pageIcon.templateData.mcq[0].option[i].feedback + '\r\r';  }}fb = '<b>Feedback</b><br>' + fb;]]></SCR>				<DIS scrolling="auto" name="feedback" x="btnSubmit._x" y="btnSubmit._y + 40" w="400" h="474 - (btnSubmit._y + btnSubmit._height) - 10" type="text"><![CDATA[{fb}]]></DIS>			</RES>		</INT>	</PG>