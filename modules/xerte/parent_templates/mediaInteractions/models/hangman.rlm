	<PG name="hangman">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();rootIcon.setTitleText(templateData.learningObject[0].name);if (pageIcon.templateData.text[0].textWidth != undefined){  icon.nextSibling.attributes.w = pageIcon.templateData.text[0].textWidth;}if (pageIcon.templateData.hangman[0].image != undefined){  fLoadImage(pageIcon.templateData.hangman[0].image);} else {  fShowMedia(pageIcon.templateData.hangman[0].showMedia);}//helpif (pageIcon.templateData.hangman[0].help != undefined){  helpStr = pageIcon.templateData.hangman[0].help;  helpIcon._visible = true;} else {  helpIcon._visible = false;}]]></SCR>		<DIS id="instr" scrolling="auto" name="text" x="380" y="20" w="400" h="475" type="text"><![CDATA[{pageIcon.templateData.hangman[0].text}]]></DIS>		<SCR name="setup"><![CDATA[words = pageIcon.templateData.hangman[0].words.split('\r');if (icon.built != 'true'){  icon.nextSibling.childNodes[1].duplicate(words.length - 1);  icon.built = 'true';}]]></SCR>		<FW id="wordsFW" name="words">			<FW_ENT name="controls">				<DIS id="otn" name="orientation" x="380" y="400" w="400" h="10" type="text"><![CDATA[{'<p align="center">Exercise 1 of ' + words.length + '</p>'}]]></DIS>				<INT name="controls" perpetual="0">					<RES enabled="false" rs="btnNext" swf="templatePath + 'common/button.swf'" name="next" type="button" x="540" y="420" label="Next" erase="1" exit="0">						<SCR name="next"><![CDATA[if (wordsFW.currentPage == words.length - 1){  wordsFW.gotoPage(0);} else {  wordsFW.nextPage();}cpad._alpha = 100;]]></SCR>					</RES>					<RES name="[Untitled]" type="event" eventName="onOpenPage" icon="wordsFW" erase="1" exit="0">						<SCR name="[Untitled]"><![CDATA[otn.setText('<p align="center">Exercise ' + (wordsFW.currentPage + 1) + ' of ' + words.length + '</p>');]]></SCR>					</RES>				</INT>			</FW_ENT>			<PG name="protoGame">				<SCR name="setup"><![CDATA[btnNext.setEnabled(false);word = words[icon.parentNode.index() - 1];dis = '';for (i = 0; i < word.length; i++){  dis = dis + '-';}usedLetters = new Object();usedStr = '';]]></SCR>				<DIS id="w" textSize="20" textFont="Courier" name="word" x="380" y="instr._y + instr._height + 30" w="400" h="10" type="text"><![CDATA[{'<b><p align="center">' + dis + '</p></b>'}]]></DIS>				<DIS id="hangGraphics" name="graphics" x="480" y="w._y + w._height + 10" type="ext" url="templatePath + 'common/hangman.swf'"><![CDATA[]]></DIS>				<DIS textFont="Courier" textSize="20" id="used" name="usedLetters" x="480" y="_ytile + 200" w="200" h="10" type="text"><![CDATA[]]></DIS>				<INT name="mainInteraction" perpetual="0">					<RES name="onKeyDown" type="event" eventName="onKeyDown" icon="rootIcon" erase="1" exit="0">						<SCR name="calculate"><![CDATA[//get key pressedl = String.fromCharCode(Key.getAscii()).toLowerCase(); found = false;if (usedLetters[l] == undefined && dis.indexOf('-') != -1 && usedStr.length < 10){    temp = '';  for (i = 0; i < dis.length; i++){    if (word.substr(i,1).toLowerCase() == l){      temp += l;      found = true;    } else {      temp += dis.substr(i,1);    }  }    dis = temp;  w.setText('<b><p align="center">' + dis + '</p></b>');    if (found == false){    usedLetters[l] = true;    usedStr += l;    used.setText(usedStr);    hangGraphics.clip.gotoAndStop(usedStr.length + 1);  }}if (dis.indexOf('_') == -1){  btnNext.setEnabled(true);  w.setText('<b><p align="center">' + dis + '</p></b>');}if (usedStr.length == 10){  btnNext.setEnabled(true);  w.setText('<b><p align="center">' + word + '</p></b>');}]]></SCR>					</RES>				</INT>			</PG>		</FW>	</PG>