	<PG name="dictation">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();if (pageIcon.templateData.dictation[0].image != undefined){  fLoadImage(pageIcon.templateData.dictation[0].image);} else {  fShowMedia(pageIcon.templateData.dictation[0].showMedia);}//helpif (pageIcon.templateData.dictation[0].help != undefined){  helpStr = pageIcon.templateData.dictation[0].help;  helpIcon._visible = true;} else {  helpIcon._visible = false;}rootIcon.setTitleText(templateData.learningObject[0].name);]]></SCR>		<DIS id="instructions" name="intructions" x="380" y="20" w="400" h="10" type="text"><![CDATA[{pageIcon.templateData.dictation[0].instructions}]]></DIS>		<SCR name="makePages"><![CDATA[if (icon.built != true){  icon.nextSibling.firstChild.duplicate(trans.p.length - 1);  icon.built = true;}]]></SCR>		<DEC name="pages" repeat="all" branch="randomUnused" pause="0" erase="0" eraseOnExit="0">			<PG name="protoPage">				<SCR name="getIndex"><![CDATA[chIndex = icon.parentNode.index();]]></SCR>				<INT name="interaction" perpetual="0">					<RES swf="templatePath + 'common/button.swf'" name="playChunk" type="button" x="380" y="instructions._y + instructions._height + 20" label="Play" erase="1" exit="0">						<SCR name="playChunk"><![CDATA[playChunk(chIndex);]]></SCR>					</RES>					<RES background="0xFFFFFF" name="ansEntry" type="textEntry" x="380" y="instructions._y + instructions._height + 60" w="400" h="100" phrase="*" scrolling="1" border="1" erase="1" exit="0" />					<RES swf="templatePath + 'common/button.swf'" name="answers" type="button" x="480" y="instructions._y + instructions._height + 20" label="Show" erase="1" exit="0">						<DIS name="answer" x="380" y="instructions._y + instructions._height + 200" w="400" h="10" type="text"><![CDATA[{'<b>Answer</b><br>' + trans.p[chIndex]}]]></DIS>					</RES>					<RES swf="templatePath + 'common/button.swf'" name="next" type="button" x="580" y="instructions._y + instructions._height + 20" label="Next" erase="1" exit="1">						<SCR name="exit"><![CDATA[stopChunk();]]></SCR>					</RES>				</INT>			</PG>		</DEC>		<DIS name="feedback" x="380" y="instructions._y + instructions._height + 20" w="400" h="10" type="text"><![CDATA[{pageIcon.templateData.dictation[0].feedback}]]></DIS>	</PG>