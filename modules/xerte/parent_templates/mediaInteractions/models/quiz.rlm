	<PG name="quiz">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();rootIcon.setTitleText(templateData.learningObject[0].name);//make questionsif (icon.attributes.built != 'true'){  icon.parentNode.getChildByName("questions").firstChild.duplicate(pageIcon.templateData.quiz[0].question.length - 1);  icon.attributes.built = 'true';}quizPageTracking = new Array();quizScoreTracking = new Array();align = pageIcon.templateData.quiz[0].align;//sequencingif (pageIcon.templateData.quiz[0].order == 'random'){  icon.parentNode.getChildByName("questions").attributes.branch = 'randomUnused';} else {  icon.parentNode.getChildByName("questions").attributes.branch = 'sequence';}panelWidth = pageIcon.templateData.quiz[0].panelWidth;if (panelWidth == "Small"){  panelW = 300;  textW = 440;} else if (panelWidth == "Medium"){  panelW = 450;  textW = 290;} else if (panelWidth == "Large"){  panelW = 600;  textW = 150;} else {  panelW = 760;  textW = 0;}if (pageIcon.templateData.quiz[0].image != undefined){  fLoadImage(pageIcon.templateData.quiz[0].image);} else {  fShowMedia(pageIcon.templateData.quiz[0].showMedia);}//helpif (pageIcon.templateData.quiz[0].help != undefined){  helpStr = pageIcon.templateData.quiz[0].help;  helpIcon._visible = true;} else {  helpIcon._visible = false;}]]></SCR>		<DIS id="instructions" name="intructions" x="380" y="20" w="400" h="10" type="text"><![CDATA[{pageIcon.templateData.quiz[0].instructions}]]></DIS>		<DEC id="quiz" name="questions" repeat="all" branch="blaH" pause="0" erase="1" eraseOnExit="0">			<PG name="protoQuestion">				<SCR name="setup"><![CDATA[if (pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].image != undefined){  prptWidth = (panelW / 2) - 30;} else {  icon.parentNode.getChildByName("loader").removeNode();  prptWidth = panelW - 40;}]]></SCR>				<DIS id="prpt" name="Prompt" x="380" y="instructions._y + instructions._height + 20" w="prptWidth" h="10" type="text"><![CDATA[{'<b>Question ' + quiz.repCount + ' of ' + quiz.pages.length + '</b><br>' + pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].prompt}]]></DIS>				<SCR name="makeOptions"><![CDATA[ln = pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].option.length;if (ln > 2 && icon.attributes.built != 'true'){  icon.nextSibling.childNodes[1].duplicate(ln - 2);  icon.attributes.built = 'true';}//multi answer?if (pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].type == 'Multiple Answer'){  for (i = 0; i < ln; i++){    icon.nextSibling.childNodes[i].attributes.type="checkBox";  }}optionTracking = new Array();for (i = 0; i < ln; i++){  optionTracking[i] = 0;  if (pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].option[i].correct == 'true'){    icon.nextSibling.childNodes[i].attributes.correct = 1;  } else {    icon.nextSibling.childNodes[i].attributes.correct = 0;  }}]]></SCR>				<INT id="ques" name="protoInteraction" perpetual="0">					<RES w="prptWidth" name="protoOption" type="radio" x="prpt._x" y="prpt._y + prpt._height + 20" label="{pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].option[icon.index()].text}" group="grp" erase="1" exit="0">						<SCR name="judge"><![CDATA[//track the questionif (pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].type == 'Single Answer'){  for (i = 0; i < ln; i++){    optionTracking[i] = 0;  }  optionTracking[icon.parentNode.index()] = 1;} if (pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].type == 'Multiple Answer'){  if (icon.parentNode.clip().getValue() == true){    optionTracking[icon.parentNode.index()] = 1;  } else {    optionTracking[icon.parentNode.index()] = 0;  }} btnSubmit.setEnabled(true);]]></SCR>					</RES>					<RES w="prptWidth" name="protoOption" type="radio" x="prpt._x" y="_ytile + 10" label="{pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].option[icon.index()].text}" group="grp" erase="1" exit="0">						<SCR name="judge"><![CDATA[//track the questionif (pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].type == 'Single Answer'){  for (i = 0; i < ln; i++){    optionTracking[i] = 0;  }  optionTracking[icon.parentNode.index()] = 1;} if (pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].type == 'Multiple Answer'){  if (icon.parentNode.clip().getValue() == true){    optionTracking[icon.parentNode.index()] = 1;  } else {    optionTracking[icon.parentNode.index()] = 0;  }} btnSubmit.setEnabled(true);]]></SCR>					</RES>					<RES toolTip="Submit" swf="templatePath + 'common/button.swf'" enabled="false" rs="btnSubmit" name="submit" type="button" x="icon.previousSibling.clip()._x" y="_ytile + 10" label="Submit" erase="1" exit="0">						<SCR name="makeFeedback"><![CDATA[//single answer feedbackfb = "";for (i = 0; i < ln; i++){  if (optionTracking[i] == 1){    fb += pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].option[i].feedback + '\r\r';  }}fb = '<b>Feedback</b><br>' + fb;if (pageIcon.templateData.quiz[0].judge == 'true'){    if (pageIcon.templateData.quiz[0].question[quiz.getPageIndex()].type == 'Single Answer'){    if (ques.judge()){      fb += 'Your answer is correct';    } else {      fb += 'Your answer is incorrect.';     }  } else {      if (ques.judge()){      fb += 'You have selected all the correct answers';    } else {      fb += 'You have not selected all the correct answers';     }  }}//track the scorm score?if (ques.judge()){  quizScoreTracking[quiz.getPageIndex()] = 1;} else {  quizScoreTracking[quiz.getPageIndex()] = 0;}]]></SCR>						<DIS scrolling="auto" name="feedback" x="btnSubmit._x" y="btnSubmit._y + btnSubmit._height + 20" w="400" h="474 - (btnSubmit._y + btnSubmit._height) - 40" type="text"><![CDATA[{fb}]]></DIS>						<SCR name="enableNext"><![CDATA[btnNext.setEnabled(true);]]></SCR>					</RES>					<RES toolTip="Next Question" enabled="false" rs="btnNext" swf="templatePath + 'common/button.swf'" name="next" type="button" x="icon.previousSibling.clip()._x" y="475" label="Next" erase="1" exit="1">						<SCR name="next"><![CDATA[//move on]]></SCR>					</RES>				</INT>			</PG>		</DEC>		<SCR name="scorm"><![CDATA[score = Math.ceil(quizScoreTracking.sum() / quizScoreTracking.length * 100);if (pageIcon.templateData.quiz[0].scorm == 'Track Last Score'){  _level0.setValue('cmi.core.score.raw', score);}if (pageIcon.templateData.quiz[0].scorm == 'Track First Score'){  if (isNaN(_level0.getValue('cmi.core.score.raw'))){    _level0.setValue('cmi.core.score.raw', score);  }}fb = 'The quiz is complete. ' + pageIcon.templateData.quiz[0].feedback;if (pageIcon.templateData.quiz[0].judge == 'true'){  fb += '<br><br>You scored ' +  quizScoreTracking.sum() + ' / ' + quizScoreTracking.length + '.';}]]></SCR>		<DIS id="finalScore" name="finalScore" x="380" y="instructions._y + instructions._height + 20" w="400" h="10" type="text"><![CDATA[{fb}]]></DIS>		<INT name="restart" perpetual="0">			<RES swf="templatePath + 'common/button.swf'" name="restart" type="button" x="finalScore._x" y="finalScore._y + finalScore._height + 20" label="Restart" erase="1" exit="0">				<SCR name="restart"><![CDATA[rootIcon.gotoPage(rootIcon.getPageIndex() - 1);]]></SCR>			</RES>		</INT>	</PG>