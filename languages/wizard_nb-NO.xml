			<prompt>Filen {f} eksisterer allerede.\r\rOverskriv denne filen?</prompt>
			<title>Bekreft</title>
		</fileexists>
	</importMedia>
	<Alert>
		<oklabel>Ok</oklabel>
		<cancellabel>Avbryt</cancellabel>
		<nolabel>Nei</nolabel>
		<yeslabel>Ja</yeslabel>
		<linkhandler>
			<prompt></prompt>
			<title>Oppgi URL for lenken</title>
		</linkhandler>
		<validate>
			<max>
				<prompt>Du kan bare legge til {m} {i}s</prompt>
				<title>Feil</title>
			</max>
			<mixedcontent>
				<prompt>Du kan ikke blande innhold her.</prompt>
				<title>Feil</title>
			</mixedcontent>
		</validate>
		<newproject>
			<prompt>Mappen er ikke tom. Er du sikker på at du vil bruke den til ditt nye prosjekt?</prompt>
			<title>Bekreft</title>
		</newproject>
		<openproject>
			<prompt>Vil du åpne det nye prosjektet?</prompt>
			<title>Bekreft</title>
		</openproject>
		<initpreview>
			<prompt>Du må velge et objekt før du kan forhåndsvise det.</prompt>
			<title>Info</title>
		</initpreview>
		<duplicateitem>
			<prompt>Du kan ikke duplisere dette objektet.</prompt>
			<title>Feil</title>
		</duplicateitem>
		<deletenode>
			<confirm>
				<prompt>Vil du slette objektet?</prompt>
				<title>Bekreftelse</title>
			</confirm>
			<error>
				<prompt>Du kan da ikke slette dette</prompt>
				<title>Feil</title>
			</error>
		</deletenode>
		<savefile>
			<prompt>XML kunne ikke bli formattert, endringer har ikke blitt lagret. Sjekk hva du nettopp endret.</prompt>
		</savefile>
        <upload>
            <successful>Opplasting fullført</successful>
            <failed>
                <permissions>Opplasting feilet: Sjekk tillatelser</permissions>
                <HTTPError>Opplasting feilet: HTTPError</HTTPError>
                <IOError>Opplasting feilet: IOError</IOError>
                <security>Opplasting feilet: Sikkerhetsfeil</security>
            </failed>
        </upload>
		
	</Alert>
