	<importMedia>
		<filters>
			<allfiles>Alle filer (*.*)</allfiles>
			<images>Bilder (*.jpg, *.jpeg, *.png)</images>
			<flash>Flashfiler (*.swf)</flash>
			<sound>Lydfiler (*.mp3)</sound>
			<video>Videofiler (*.flv)</video>
		</filters>
		<title>Velg fil som skal importeres...</title>
		<filter>Alle filer|*.*|JPG|*.jpg|PNG|*.png|SWF Files|*.swf|Sounds|*.mp3|Video|*.flv</filter>
		<btnimport>Importer</btnimport>
		<btncancel>Avbryt</btncancel>
